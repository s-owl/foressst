[
  {% assign feed = site.posts limit:20 %}

  {% for post in feed %}
  {
  "title" : "{{ post.title }}",
  "date" : "{{ post.date | date: "%Y.%m.%d" }}",
  "content" : {{ post.content | strip_html | jsonify }}
  }
  {% if post != feed.last %}
  ,
  {% endif %}
  {% endfor %}
]
